{
  "name": "@bauer-group/n8n-nodes-http-throttled-request",
  "version": "0.1.1",
  "description": "n8n community node with built-in HTTP request throttling (rate limiting)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bauer-group/EXT-n8n-httpThrottled.git"
  },
  "main": "dist/nodes/HttpRequest/HttpRequestThrottled.node.js",
  "n8n": {
    "nodes": [
      {
        "file": "dist/nodes/HttpRequest/HttpRequestThrottled.node.js"
      }
    ]
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0",
    "n8n-core": ">=1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^25.3.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "n8n-workflow": "^1.67.0",
    "n8n-core": "^1.67.0",
    "rimraf": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    }
  }
}
